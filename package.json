{
  "name": "phaser-project-template",
  "version": "3.87.0",
  "description": "Phaser 3.87.0 starter template with TypeScript and webpack.",
  "homepage": "https://devery06.github.io/chaoshunter",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --config webpack/webpack.dev.js",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
    "bundle": "npm run build",
    "serve": "serve dist",
    "format": "prettier --check src/scripts/**",
    "format:write": "prettier --write src/scripts/**",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public -r https://github.com/devery06/Chaos-Hunter.git"
  },
  "keywords": [
    "Phaser",
    "Phaser 3",
    "Phaser3",
    "html5 game",
    "TypeScript",
    "webpack",
    "starter"
  ],
  "author": {
    "name": "Yannick Deubel",
    "url": "https://github.com/yandeu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yandeu/phaser-project-template.git"
  },
  "template": {
    "name": "phaser-project-template",
    "url": "https://github.com/yandeu/phaser-project-template",
    "author": "Yannick Deubel (https://github.com/yandeu)"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/webfontloader": "^1.6.38",
    "@yandeu/prettier-config": "^0.0.4",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.6.3",
    "javascript-obfuscator": "^4.1.1",
    "mime-types": "^2.1.35",
    "mini-css-extract-plugin": "^2.9.2",
    "path": "^0.12.7",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "serve": "^14.2.4",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.1",
    "webfontloader": "^1.6.28",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1",
    "webpack-obfuscator": "^3.5.1"
  },
  "dependencies": {
    "phaser": "^3.87.0",
    "pick-random-weighted": "2.0.0"
  }
}
